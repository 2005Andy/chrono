include "Agent.fbs";
include "SPAT.fbs";
include "MAP.fbs";
include "Terrain.fbs";
include "Sensor.fbs";
include "Approach.fbs";

namespace SynFlatBuffers;

union Type { 
  Agent.State,
  Agent.Description, 
  SPAT.State, 
  MAP.State, 
  Terrain.State, 
  Sensor.State,
  Approach.State
}

table Buffer {
  buffer:[Message];
}

